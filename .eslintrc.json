{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules" : {
        "no-console": "off",
        "warn":"off",
        "quotes": [
            "error",
            "double"
        ],
        "indent": ["error", 4],
        "linebreak-style": 0,
        "prefer-const" : ["error", {
            "destructuring": "any",
            "ignoreReadBeforeAssign": false
        }]
    }
}
